# Copy this file to .env and fill in your values

# Private key for deployment (with 0x prefix)
PRIVATE_KEY=0xyour_private_key_here

# Arbitrum RPC URLs (optional - defaults work fine)
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Arbiscan API key for contract verification (required for open source verification)
# Get your free API key at: https://arbiscan.io/myapikey
ARBISCAN_API_KEY=your_arbiscan_api_key_here


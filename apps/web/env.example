# WalletConnect Project ID (get one at https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Optional: Alchemy API key for better RPC performance
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_key_here

# REQUIRED: Claimable contract addresses (must be set before production use)
# SECURITY WARNING: Do not use zero addresses in production - funds will be lost!
NEXT_PUBLIC_CLAIMABLE_ADDRESS_ARBITRUM=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_CLAIMABLE_ADDRESS_ARBITRUM_SEPOLIA=0x0000000000000000000000000000000000000000


